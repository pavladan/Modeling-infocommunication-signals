{
    "appId": "com.modeling-infocommunication-signals.app",
    "productName": "Modeling Infocommunication Signals",
    "directories": {
        "output": "release/"
    },
    "asar": true,
    "asarUnpack": [],
    "files": [
        "dist/**/*",
        "main.js",
        "node_modules/**/*"
    ],
    "win": {
        "icon": "public/favicon.ico",
        "target": ["nsis"]
    },
    "mac": {
        "icon": "public/favicon.png",
        "target": ["dmg"]
    },
    "linux": {
        "icon": "dist",
        "target": ["AppImage"]
		},
		"nsis": {
			"oneClick": false,
			"perMachine": true,
			"allowToChangeInstallationDirectory": true
		},
    "extends": null
}
